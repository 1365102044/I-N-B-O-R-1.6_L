@implementation YXContentTextField

- (void)awakeFromNib
{
    [super awakeFromNib];
    UIView *leftView = [[UIView alloc] initWithFrame:CGRectMake(0, 0, 17, self.frame.size.height)];
    self.leftViewMode = UITextFieldViewModeAlways;
    self.leftView = leftView;
}

@end
